{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard-with-typescript",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "project": ["./tsconfig.json", "./tsconfig.eslint.json"],
        "sourceType": "module"
    },
    "rules": {
        "@typescript-eslint/quotes": [2, "double", { "avoidEscape": true }],
        "@typescript-eslint/semi": [2, "always"]
    }
}
