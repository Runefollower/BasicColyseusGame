{
  "compilerOptions": {
    "outDir": "./dist/server",
    "module": "CommonJS",
    "target": "es2019",
    "declaration": true,
    "removeComments": true,
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "strict": true,
    "allowJs": true,
    "strictNullChecks": false,
    "forceConsistentCasingInFileNames": true,
    //"moduleResolution": "Node",
    "lib": ["dom", "ES2019"]
  },
  "include": ["src/server"]
}
